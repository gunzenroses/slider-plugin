{
  "name": "jq-slider-mvc",
  "version": "1.0.0",
  "home": "https://github.com/gunzenroses/slider-plugin",
  "description": "jQuery plugin range-slider with MVC structure",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage --watchAll=false --verbose=false",
    "dev": "webpack serve --config ./webpack.dev.js --env development --progress --live-reload",
    "prod": "rimraf dist && webpack --config ./webpack.prod.js --env production --mode=production",
    "build": "rimraf build && webpack --config ./webpack.build.js --env production --mode=production",
    "deploy": "gh-pages -d build",
    "predeploy": "npm run build",
    "docs": "jsdoc2md --template ./README.hbs --files ./src/lib/*.ts --configure ./jsdoc2md.json > ./README.md",
    "eslint": "eslint --ext .ts,.tsx src --fix",
    "debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gunzenroses/slider-plugin.git"
  },
  "keywords": [
    "jQuery",
    "plugin",
    "range-slider",
    "MVC"
  ],
  "author": "@gunzenroses",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gunzenroses/slider-plugin/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/plugin-proposal-decorators": "^7.16.7",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-typescript": "^7.13.0",
    "@types/jest": "^27.4.0",
    "@types/jquery": "^3.5.13",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "autobind-decorator": "^2.4.0",
    "autoprefixer": "^10.4.2",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "canvas": "^2.8.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^8.1.1",
    "css-loader": "^5.2.4",
    "cssnano": "^5.0.8",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-fsd": "^1.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^24.4.0",
    "file-loader": "^6.2.0",
    "gh-pages": "^3.2.3",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^26.6.3",
    "jest-environment-jsdom-fifteen": "^1.0.2",
    "mini-css-extract-plugin": "^1.6.0",
    "node-sass": "^7.0.1",
    "postcss": "^8.4.5",
    "postcss-loader": "^5.2.0",
    "postcss-preset-env": "^7.2.3",
    "pug": "^3.0.2",
    "pug-loader": "^2.4.0",
    "sass": "^1.32.12",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.1.2",
    "typescript": "^4.4.4",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.0.0"
  },
  "dependencies": {
    "jquery": "^3.6.0"
  }
}
