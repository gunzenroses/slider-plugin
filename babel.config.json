{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "module-resolver",
      { 
        "alias": {
          "assets": "./src/assets/",
          "mvp": "./src/components/mvp/",
          "Panel": "./src/components/panel/",
          "Interfaces": "./src/components/interfaces/",
          "Helpers": "./src/scripts/helpers/",
          "Utils": "./src/scripts/utils/"
        }
      }
    ]
  ]
}